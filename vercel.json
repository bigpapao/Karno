{
  "version": 2,
  "name": "karno-ecommerce",
  "buildCommand": "cd karno/frontend && npm install && npm run build",
  "outputDirectory": "karno/frontend/build",
  "installCommand": "npm run install:all",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 